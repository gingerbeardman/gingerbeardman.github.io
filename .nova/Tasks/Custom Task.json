{
  "actions" : {
    "build" : {
      "enabled" : true,
      "script" : "bundle exec jekyll serve --drafts --profile --open-url --livereload"
    },
    "run" : {
      "enabled" : true,
      "script" : "bundle exec jekyll serve --incremental --drafts --profile --open-url --livereload"
    }
  },
  "openLogOnRun" : "start"
}
